<!DOCTYPE html>
<html>
<head>
  <title>Encore</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<a href="<%= root_url %>">ENCORE</a>
<% if logged_in? %>
<%= render partial: 'layouts/header_logged_in' %>
<% else %>
<%= render partial: 'layouts/header_guest' %>
<% end %>
<%= yield %>

</body>
</html>
