<!DOCTYPE html>
<html>
<head>
  <title>Encore</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body class="black-home">
<% if logged_in? %>
<%= render partial: 'layouts/navbar_logged_in' %>
<% else %>
<%= render partial: 'layouts/navbar_guest' %>
<% end %>
<%= flash[:errors] if flash[:errors] %>

<%= yield %>
</body>
</html>
